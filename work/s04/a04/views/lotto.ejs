        </tr>
      </table>
      </div>
      <p class="lotto_text">Total matches: <%= correctNumbers %>.<p>
      <script type="text/javascript">
        var draw = [<%= drwanNumbers %>];
        console.log(draw);
        var table = document.getElementById('draw');
        var tds = table.getElementsByTagName('td');
        for (let i = 0; i < tds.length; i++) {
            tds[i].innerHTML = draw[i];
        }
        var player = [<%= ownNumbers %>];
        console.log(player);
        var tablePlayer = document.getElementById('player');
        var tdsPlayer = tablePlayer.getElementsByTagName('td');
        for (let i = 0; i < tdsPlayer.length; i++) {
            tdsPlayer[i].innerHTML = player[i];
        }
      </script>

    </body>
